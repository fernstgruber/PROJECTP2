@Article{Marchi2005,
  Title                    = {GIS morphometric indicators for the analysis of sediment dynamics in mountain basins},
  Author                   = {Marchi, Lorenzo and Dalla Fontana, Giancarlo},
  Journal                  = {Environmental Geology},
  Year                     = {2005},
  Number                   = {2},
  Pages                    = {218--228},
  Volume                   = {48},

  __markedentry            = {[fabs:]},
  Abstract                 = {Watershed management and headwater reconstruction programs require a reliable knowledge of sediment dynamics. Geographical Information Systems (GIS) provide the framework for the implementation of different complex techniques for the assessment of shallow landsliding and erosion processes in mountain basins. This paper presents some morphometric indicators aimed at erosion and sediment delivery analysis. The proposed indicators can be easily derived from medium to fine resolution Digital Elevation Models (DEM). Applications conducted in Eastern Italian Alps have shown the adequateness of the proposed approach to address erosion and sediment-related problems. The analysis considered the classification of sediment source areas with regard to their activity, the comparison between drainage basins having different morphological characteristics and the topographic control on sediment transport capacity, with a particular attention to the identification of channel reaches characterised by a low sediment transport capacity.},
  Doi                      = {10.1007/s00254-005-1292-4},
  File                     = {:/media/fabs/Volume/Data/literature/Diss/Marchi and Fontana 2004.pdf:PDF},
  ISSN                     = {1432-0495},
  Owner                    = {fabs},
  Timestamp                = {2016.09.15},
  Url                      = {http://dx.doi.org/10.1007/s00254-005-1292-4}
}

@Article{McBratney2003,
  Title                    = {On digital soil mapping},
  Author                   = {Alex B. McBratney and M.L. {Mendonca Santos} and B. Minasny},
  Journal                  = {Geoderma},
  Year                     = {2003},
  Pages                    = {3 - 52},
  Volume                   = {117},

  File                     = {:/media/fabs/Volume/Data/literature/Diss/mcbratney_2003.pdf:PDF},
  Owner                    = {read},
  Review                   = {describes the resolution and coverage of soil maps in different countries outline the framework of a empirical partially deterministic partially stochastic model: soil spatial prediction functions (SSPFe) with spatially autocorrelated errors Brief review of approaches (#) to soil spatial prediction: #Jenny ( lot of this early quantitative work was very well summarised by Yaalon (1975), mainly to understand soil formation, not for predicitve mapping), #geographich and neighbourhood approaches (purely spatial) as well as geostatistics ( because of a failure to obtain prediction from the soil-forming factors largely because the quantitative variables describing these factors were not readily available to do such predictions, trend-surfaces applied to soil attributes such as pH, T°C; kriging also estimates uncertainies;co-kriging applies denser sets of secondary variables (other attributes of environmental factors) corelated with primary variable; Environmental correlation approach (Jenny + geography) clorpt-formula for quantitave prediction: using elevation derivatives to predict eg depth of A-horizon; subsets were used to predict soil classes), similarity to co-kriging lead to regression kriging (cloprt for prediction of soil attributes,kriging used on residuals) #predicting attributes from other soil attibutes: philips 2001 shows clorpt not working esp at fine resolutions - soil (other attributes at same spot) must be added to clorpt list SCORPAN:Jenny-like formulation not for explanation but for EMPIRICAL quantitative descriptions of relationships between soil and other spatially referenced factors, each factor represented by a set of continous or categorical values, not considering direction of causality. The model shall predict probabilties with a measure of uncertainties. does not consider pedotransfer functions (s=f(s)) with spatial reference. (read Heuvelink1996 for his regresseion kriging and 1wayANOVA) . The function is fitted to the observation points and then extended to the whole grid POSSIBLE FUNCTIONS (#) #linear models:--ordinary least squares, --principles component regression (for large number of correlated predictor variables) and partial least squares (components are determined by response AND predictor variables, used esp for electromagnetic spectrum, --linear discriminant analysis: seminal supervised learning technique;IMPORTANT: used by Bell et al. (1992, 1994) who related soil drainage classes to landscape parameters #Generalized linear model: --can handle nonnormal response distributions, modifyingmodel instead of transforming variables (also co-kriging is GLM). better than neural networks and regression trees for predicting soil attributes from environmental variables. #Generalized additive models:try to characterize nonlinear effects not considered in GLMs. not used much in soil science #Tree models-classification and regression: rule-structured classifiers built from training sets. can deal with nonlinearity, no assumptions needed, can deal with nonadditive behaviour.eg.CART. easily interpreted, widely used for prediction of soil attributes. --prediction of continuous attributes with regression tree. discrete predictions from each terminal node -> not very smooth when small nr of terminal nodes. possible improvement = multivariate lm at each node, or fuzzy membership. --prediction of soil classe =with decision/classification tree. CART=binary decision tree. used for mapping Murray-Darling basin #Neural networks:sytem of many elements with communication channels (connectors, which are weighted). networks are trained to a specific target output. Used lots, esp for pedotransfer functions, also applied to soil classes #Fuzzy systems: attempt to represent the uncertainty in predictors and predicted classes. hard input is fuzzified, fuzzy operators defined, defuzzify output. #Other models: genetic algorithms (randomised search and optimisation techniques), from data mining: multivariate adaptive regression splines(MARS), treenet (analogous to long series expansion(fourier)) #Strengthening models: bootstrap aggregating - assess acuracy of prediction with bootstrapping, model is fit for each set. -boosting:combines outut of many 'weak models' whch are weighted and then aggregated #Expert systems: Bui2003: soil maps are representations of the surveyors mental soil-landscape model by structuring terrain objects in a nested hierarchy followed by inference and formalisation of knowledge rules. eg Expector approach, using elevation derivatives #Unsupervised classification: instead of placing soil into existing classes, these classes are produced from observed soil properties. Big in 1960s, main rules are k-means and fuzzy k-means. afterwards one of the previous methods is used to fit model SPATIAL CONSIDERATIONS : considering spatial components of the environmental and soil variables and by perpending the spatial correlation strucuture of the residuals. decomposing the predictor variables into seperate spatial components and mapped seperately, with factorial kriging analysis or wavelets, the former assuming stationarity and finding the scale of the components from observations. Very likely short spatial range components(nuggget) don't relate to soil and can be removed. Structure in e-generalised least squares and geostatistics - variograms of the fitted parts of prediction functions may be instructive #GLS: s=Qb+e estimate parameter vector, calculate e , their semivariance, fit semivariogram, calculate correlation matrix, calculate R, iterate until R=min #geostatistics - scorpan-kriging: spatial trend is described by scorpan and residuals e modeled by variograms and some sort of kriging; --scorpan-universal kriging:allows incorporation of deterministic trend (bq(x), which can only be linear function) and stochastic components (e(x), modeled with variogram) in kriging. Alternately kriging is combined with regression to model the trend function seperately s(x)=f(q,x)+e'(x) ; In regression kriging, the soil property S at unvisited site is first predicted by f(), and followed by kriging of the residuals of the model. --scorpan-simple-kriging: under assumption that e has mean=0, simple kriging allows.... versteh i ned --scorpan compositional kriging: for prediction of soil classes incorporating predictor variables Q, a form of compositional krigiing with external trend is proposed. The probabilites of belonging to a soil class must sum up to one, and residuals to 0 # geostatistics - co-kriging and coregionalisation analysis: often used, any q layer can be covariate.however, linear relation between predictor and variable is assumed; regressionkriging seems to perform better (Odeh1995). --coregionalisation: even without cokriging it helps understand linear spatial relationships #other methods: --Bayesian maximum entropy allows incorporation of wide variety of soft and hard data. compared to ordinary kriging it uses soft data on a sound theoretical basis. --Fractal interpolation: effort to preserve spatial variability of original data when transferred across scales (Bindlish and Barros1996 used it for elevation data (IINNTTEERREESSSSAANNTT!!) PREVIOUS SCORPAN STUDIES: The key predictor factors are r (80% of studies) followed by s (35%), o and p (both 25%), n (20%) and c (5%) whereas a does not seem to have been used as a factor. No studies considered five or more factors. DATA SOURCES: #s: -surface multi-and hyperreflectance, -radar attenuation, -electrical conductivity, -gamma radiometrics, -preexisting soil class or properties map #c: -temperature, -precipitation, -evapotranspiration, -water balance components #o: -vegation map, -landcover and landuse classification, -biomass and yield maps, #r. -primary terrain attributes (slope, aspect, curvature and upslope contributing area), local or regional, scale-specific or -invariant (Shary2002), -secondary attributes (eg wetness index), -terrain or landscape classification (Pennock et al 1987, Wood1996, MacMillan2000 and others) #p: geological and regolith maps (in australia from hyperspectral data and expert knowledge) #a: difficult #m: simply coordinates to ensure spatial considerations ScorpanSSPFe-Method:: 1) Define soil attribute(s) of interest and decide resolution q and block size b (20-200 m) 2) Assemble data layers to represent Q 3) Spatial decomposition or lagging of data layers. wavelet decomposition or : geostatistically by modelling the correlation structure in the imagery by decomposing the variogram into independent spatial components, and then taking each component in turn and kriging it, thereby separating it from the others. Both methods get rid of short-range uncorrelated noise components 4) Sampling of assembled data (Q) to obtain sampling sites : aim is to construct predictive equations for the soil attributes of interest in terms of the environmental variables. One possibility is to use Latin hypercube sampling (McKay et al., 1979), creating tuplets with characteristic combinations. At points that are similar to these tuplets soil samples schould be taken in order to consider different characteristics. Hengl has a similar metobergurgl 12.Julihod. Or sample a area that resembles the greater region. 5) GPS field sampling and laboratory analysis to obtain soil class or property data. sometimes duplicate in the range of 1/2 grid to check for shortrange variability 6) Fit quantitative relationships (observing Ockham’s razor) - (viele viele algorithmen)- is a single model adequate/sufficient? 7) Predict digital map 8) Field sampling and laboratory analysis for corroboration and quality testing - local variation and uncertainty in environmental layers will lead to error; required to provide an independent estimate of the quality of the map produced ; Marsman and de Gruijter (1986) approach is more efficient in that the sampling is designed specifically for corroboration. little work on corroboration of digital soil maps, especially of classes. This is an area of research need. 9)If necessary simplify legend or decrease resolution by returning to (i) or improve map by returning to (v) USES described discussion: why not call SSPFe a pedotransferfunction? DANGERS VIEL VIEL LITERATUR!},
  Timestamp                = {2008.11.21}
}

